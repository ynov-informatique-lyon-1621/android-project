        tools:layout_editor_absoluteX="138dp"
        tools:layout_editor_absoluteY="168dp" />

    <ImageView
        android:id="@+id/imagevendeursucces"
        android:layout_width="71dp"
        android:layout_height="68dp"
        app:srcCompat="@drawable/noun"
        tools:layout_editor_absoluteX="312dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/vendeursucces"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nom vendeur"
        tools:layout_editor_absoluteX="309dp"
        tools:layout_editor_absoluteY="106dp" />

    <Button
        android:id="@+id/annoncesucces"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Retour à l'annonce"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="433dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Retour à votre recherche"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="373dp" />
</android.support.constraint.ConstraintLayout>