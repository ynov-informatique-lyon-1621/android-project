<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI.ContacterVendeurActivity">

    <ImageView
        android:id="@+id/imageContact"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="30dp"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="42dp" />

    <TextView
        android:id="@+id/titreConfirm"
        android:layout_width="265dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="30dp"
        android:text="TextView"
        android:textColor="@android:color/black"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="44dp" />

    <TextView
        android:id="@+id/vendeurContact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/prixConfirm"
        android:layout_toEndOf="@+id/messageContact"
        android:text="TextView"
        android:textColor="@android:color/black"
        tools:layout_editor_absoluteX="288dp"
        tools:layout_editor_absoluteY="61dp" />

    <TextView
        android:id="@+id/prixConfirm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="60dp"
        android:layout_toStartOf="@+id/retourContact"
        android:text="TextView"
        tools:layout_editor_absoluteX="208dp"
        tools:layout_editor_absoluteY="70dp" />

    <TextView
        android:id="@+id/catContact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="90dp"
        android:text="TextView"
        android:textColor="@android:color/black"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="93dp" />

    <EditText
        android:id="@+id/nomContact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="160dp"
        android:ems="10"
        android:hint="Nom*"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="202dp" />

    <EditText
        android:id="@+id/emailContact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="219dp"
        android:ems="10"
        android:hint="Email*"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="265dp" />

    <EditText
        android:id="@+id/telContact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="270dp"
        android:ems="10"
        android:hint="NumÃ©ro Telephone"
        android:inputType="number"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="136dp" />

    <EditText
        android:id="@+id/messageContact"
        android:layout_width="217dp"
        android:layout_height="82dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="320dp"
        android:ems="10"
        android:hint="Message*"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="328dp" />

    <Button
        android:id="@+id/validerContact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/resetContact"
        android:layout_alignParentStart="true"
        android:layout_marginStart="13dp"
        android:text="valider"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="420dp" />

    <Button
        android:id="@+id/resetContact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="52dp"
        android:text="reset"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="420dp" />

    <Button
        android:id="@+id/retourContact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/validerContact"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="16dp"
        android:text="retour"
        tools:layout_editor_absoluteX="266dp"
        tools:layout_editor_absoluteY="420dp" />

    <TextView
        android:id="@+id/dateConfirm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/prixConfirm"
        android:layout_alignParentTop="true"
        android:layout_marginTop="122dp"
        android:text="TextView"
        android:textColor="@android:color/black"
        tools:layout_editor_absoluteX="208dp"
        tools:layout_editor_absoluteY="93dp" />
</RelativeLayout>